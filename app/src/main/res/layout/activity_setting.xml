        android:paddingBottom="5dp"

        />
    <com.gaoyehua.ui.SettingView
        android:id="@+id/sv_setting_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        ></com.gaoyehua.ui.SettingView>

</LinearLayout>
